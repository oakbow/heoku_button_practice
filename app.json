{
  "name": "Heroku Static Provider",
  "description": "Static site provider for Heroku.",
  "keywords": [
    "static",
    "site"
  ],
  "website": "http://www.ok-sky.jp",
  "repository": "https://github.com/OKSKY/oksky-chat.git",
  "logo": "https://raw.githubusercontent.com/OKSKY/oksky-chat/master/public/img/oksky-logo-mini.png?token=AI0inHoUYDdC5gF4EYxqJosEEHbwx_vSks5XQpvTwA%3D%3D",
  "success_url": "/",
  "env": {
    "USER": {
      "description": "(optional) The username of Basic Auth.",
      "value": "",
      "required": false
    },
    "PASS": {
      "description": "(optional) The password of Basic Auth.",
      "value": "",
      "required": false
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
  },
  "image": "heroku/ruby",
  "addons": [
    "papertrail:choklad",
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "9.5"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}